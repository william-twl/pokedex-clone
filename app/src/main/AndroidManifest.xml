<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.william.pokedex_clone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_pokeball"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_pokeball"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pokedexclone">
        <activity android:name="com.william.pokedex_clone.module.splash.activity.SplashActivity"
            android:theme="@style/Theme.Design.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.william.pokedex_clone.module.main.activity.MainActivity"/>
    </application>

</manifest>